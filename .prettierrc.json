{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 140,
  "singleQuote": true,
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "bracketSameLine": true,
  "htmlWhitespaceSensitivity": "css",
  "singleAttributePerLine": true,
  "arrowParens": "always",
  "endOfLine": "crlf"
}
